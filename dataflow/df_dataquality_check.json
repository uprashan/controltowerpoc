{
	"name": "df_dataquality_check",
	"properties": {
		"description": "df_merge_po_tables",
		"folder": {
			"name": "Control_Tower"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "PO_Excel_Files",
						"type": "DatasetReference"
					},
					"name": "opstream"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false,",
				"     enableCdc: true,",
				"     mode: 'read',",
				"     skipInitialLoad: false,",
				"     dateFormats: ['MM-dd-yyyy'],",
				"     timestampFormats: ['yyyy-MM-dd\\'T\\'HH:mm:ss'],",
				"     preferredIntegralType: 'integer',",
				"     preferredFractionalType: 'double') ~> opstream"
			]
		}
	}
}