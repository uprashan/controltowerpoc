{
	"name": "df_dataquality_check_copy2",
	"properties": {
		"description": "df_merge_po_tables",
		"folder": {
			"name": "Control_Tower"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Controltower_ds",
						"type": "DatasetReference"
					},
					"name": "opstreamdqfile"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					},
					"name": "outputsqlintelmodel"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "sinkapprovedpofiles",
						"type": "DatasetReference"
					},
					"name": "storagepurchaseorders"
				}
			],
			"transformations": [
				{
					"name": "lookup1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          REPORT_DATE as date,",
				"          {Subcon ID} as string,",
				"          {Subcon Site} as string,",
				"          CUSTOMER as string,",
				"          SUPPLIER as string,",
				"          PRODUCT_NAME as string,",
				"          ITEM_NUMBER as string,",
				"          STD_COST as string,",
				"          MFG as string,",
				"          ITEM_DESCRIPTION as string,",
				"          PO_CREATION_DATE as date,",
				"          LINE_NUMBER as string,",
				"          SHIPMENT_NUM as string,",
				"          QUANTITY as string,",
				"          QTY_RECEIVED as double,",
				"          {In receiving Qty} as string,",
				"          REMAINING_QTY as string,",
				"          ETD_DATE as date,",
				"          PROMISED_DATE as string,",
				"          LEAD_TIME as string,",
				"          TRANSIT_LEAD_TIME as string,",
				"          POSTPROCESSING_LEAD_TIME as string,",
				"          NCNR as string,",
				"          MOQ as string,",
				"          FIXED_LOT_MULTIPLIER as string,",
				"          PO_NUMBER as string,",
				"          RELEASE_NUM as string,",
				"          {Recommend ETD Date} as date,",
				"          PO_TYPE as string,",
				"          MANUFACTURER as string,",
				"          MFG_PART as string,",
				"          CRITICAL_SHORTAGE_FLAG as string,",
				"          DISTI_INDICATOR as string,",
				"          SUPPLIER_ADDRESS as string,",
				"          PROMISED_QUANTITY as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> opstreamdqfile",
				"source(output(",
				"          Business_Unit_ID as string,",
				"          Component_Supplier_ID as string,",
				"          Component_Supplier_Name as string,",
				"          Component_Unit_Pricing as double,",
				"          Critical_Shortage_Flag as string,",
				"          Distributor_ID as string,",
				"          Distributor_Name as string,",
				"          IDMI_PO as string,",
				"          Intel_Part_Number as string,",
				"          Manufacturer_ID as string,",
				"          Intel_Part_Number_IDENTIFIER as string,",
				"          Manufacturer_Name as string,",
				"          Manufacturing_Part_Number as string,",
				"          ODM_Part_Number as string,",
				"          Order_Ship_From_Location as string,",
				"          PO_creation_Date as date,",
				"          PO_Number as string,",
				"          PO_Schedule_Line_Item as string,",
				"          Report_Date as date,",
				"          Shipment_Receipt_Date as date,",
				"          Shipment_Receipt_Quantity as double,",
				"          Subcon_Site as string,",
				"          Subcon_ID as string,",
				"          Target_LeadTime as string,",
				"          Release_Number as string,",
				"          PO_Type as string,",
				"          Disti_Indicator as string,",
				"          Receipt_Number as string,",
				"          Cancel_Date as date,",
				"          Closed_Date as date,",
				"          PO_Line_Item as string,",
				"          IPN_Description as string,",
				"          Address_For_Component_Supplier as string,",
				"          Address_For_Disti as string,",
				"          NCNR as string,",
				"          Supplier_MOQ as string,",
				"          PTP as string,",
				"          Supplier_Allocation as string,",
				"          Custom_or_Standard as string,",
				"          Transit_Lead_Time as string,",
				"          Postprocessing_Lead_Time as string,",
				"          Tier2_PO_Number as string,",
				"          Tier2_PO_Line_Number as string,",
				"          Tier2_PO_Schedule_Line_Item as string,",
				"          Tier2_PO_Date as date,",
				"          Tier2_PO_Request_Date_RDD as date,",
				"          Tier2_PO_Request_Quantity as string,",
				"          Tier2_PO_Commit_Date_ETD as date,",
				"          Tier2_PO_Commit_Date_ETA as date,",
				"          Tier2_Shipment_Receipt_Date_GR as date,",
				"          Tier2_Shipment_Receipt_Quantity as string,",
				"          Tier2_Intel_Part_Number_Identifier as string,",
				"          Tier2_Sales_Order_NO as string,",
				"          Tier2_Sales_Order_Line_Item as string,",
				"          Tier2_Sales_Order_Quantity as string,",
				"          Tier2_Target_Leadtime_Day as string,",
				"          PO_Commit_Quantity as double,",
				"          PO_Commit_Date as date,",
				"          PO_Request_Date as date,",
				"          PO_Request_Quantity as double,",
				"          Remaining_Open_PO_Quantity as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> outputsqlintelmodel",
				"opstreamdqfile, outputsqlintelmodel lookup(CUSTOMER == Business_Unit_ID",
				"     && SUPPLIER == Component_Supplier_Name",
				"     && opstreamdqfile@CRITICAL_SHORTAGE_FLAG == outputsqlintelmodel@Critical_Shortage_Flag",
				"     && SUPPLIER == Distributor_Name",
				"     && opstreamdqfile@DISTI_INDICATOR == IDMI_PO",
				"     && ITEM_NUMBER == Intel_Part_Number",
				"     && CUSTOMER == Intel_Part_Number_IDENTIFIER",
				"     && MANUFACTURER == Manufacturer_Name",
				"     && MFG_PART == Manufacturing_Part_Number",
				"     && ITEM_NUMBER == ODM_Part_Number",
				"     && LINE_NUMBER == PO_Line_Item",
				"     && opstreamdqfile@PO_CREATION_DATE == outputsqlintelmodel@PO_creation_Date",
				"     && opstreamdqfile@PO_NUMBER == outputsqlintelmodel@PO_Number",
				"     && SHIPMENT_NUM == PO_Schedule_Line_Item",
				"     && opstreamdqfile@REPORT_DATE == outputsqlintelmodel@Report_Date",
				"     && QTY_RECEIVED == Shipment_Receipt_Quantity",
				"     && {Subcon Site} == Subcon_Site",
				"     && {Subcon ID} == Subcon_ID",
				"     && LEAD_TIME == Target_LeadTime",
				"     && RELEASE_NUM == Release_Number",
				"     && opstreamdqfile@PO_TYPE == outputsqlintelmodel@PO_Type",
				"     && opstreamdqfile@DISTI_INDICATOR == outputsqlintelmodel@Disti_Indicator",
				"     && LINE_NUMBER == PO_Line_Item",
				"     && ITEM_DESCRIPTION == IPN_Description",
				"     && SUPPLIER_ADDRESS == Address_For_Component_Supplier",
				"     && SUPPLIER_ADDRESS == Address_For_Disti",
				"     && opstreamdqfile@NCNR == outputsqlintelmodel@NCNR",
				"     && MOQ == Supplier_MOQ",
				"     && opstreamdqfile@TRANSIT_LEAD_TIME == outputsqlintelmodel@Transit_Lead_Time",
				"     && opstreamdqfile@POSTPROCESSING_LEAD_TIME == outputsqlintelmodel@Postprocessing_Lead_Time",
				"     && opstreamdqfile@PO_NUMBER == Tier2_PO_Number,",
				"     multiple: true,",
				"     broadcast: 'auto')~> lookup1",
				"lookup1 select(mapColumn(",
				"          Business_Unit_ID = CUSTOMER,",
				"          Component_Supplier_Name = SUPPLIER,",
				"          Critical_Shortage_Flag = opstreamdqfile@CRITICAL_SHORTAGE_FLAG,",
				"          IDMI_PO = opstreamdqfile@DISTI_INDICATOR,",
				"          Distributor_Name = SUPPLIER,",
				"          Intel_Part_Number = ITEM_NUMBER,",
				"          Intel_Part_Number_IDENTIFIER = CUSTOMER,",
				"          Manufacturer_Name = MANUFACTURER,",
				"          Manufacturing_Part_Number = MFG_PART,",
				"          ODM_Part_Number = ITEM_NUMBER,",
				"          PO_Line_Itemn = LINE_NUMBER,",
				"          PO_creation_Date = opstreamdqfile@PO_CREATION_DATE,",
				"          PO_Number = opstreamdqfile@PO_NUMBER,",
				"          PO_Schedule_Line_Item = SHIPMENT_NUM,",
				"          Report_Date = opstreamdqfile@REPORT_DATE,",
				"          Shipment_Receipt_Quantity = QTY_RECEIVED,",
				"          Subcon_Site = {Subcon Site},",
				"          Subcon_ID = {Subcon ID},",
				"          Target_LeadTime = LEAD_TIME,",
				"          Release_Number = RELEASE_NUM,",
				"          PO_Type = opstreamdqfile@PO_TYPE,",
				"          Disti_Indicator = opstreamdqfile@DISTI_INDICATOR,",
				"          PO_Line_Item = LINE_NUMBER,",
				"          IPN_Description = ITEM_DESCRIPTION,",
				"          Address_For_Component_Supplier = SUPPLIER_ADDRESS,",
				"          Address_For_Disti = SUPPLIER_ADDRESS,",
				"          NCNR = opstreamdqfile@NCNR,",
				"          Supplier_MOQ = MOQ,",
				"          Transit_Lead_Time = opstreamdqfile@TRANSIT_LEAD_TIME,",
				"          Postprocessing_Lead_Time = opstreamdqfile@POSTPROCESSING_LEAD_TIME,",
				"          Tier2_PO_Number = opstreamdqfile@PO_NUMBER",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     input(",
				"          Column_1 as string",
				"     ),",
				"     partitionFileNames:['po_fabrinet_intel_model.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> storagepurchaseorders"
			]
		}
	}
}