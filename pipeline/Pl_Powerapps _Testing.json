{
	"name": "Pl_Powerapps _Testing",
	"properties": {
		"activities": [
			{
				"name": "Copy Powerapps to Azure Blob",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "CommonDataServiceForAppsSource"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": false,
					"translator": {
						"value": "@json('{\\\"type\\\":\\\"ColumnTranslator\\\",\\\"mappings\\\":[{\\\"source\\\":{\\\"name\\\":\\\"craf2_idmipo\\\"},\\\"sink\\\":{\\\"name\\\":\\\"IDMI PO\\\"}}].json_output}')",
						"type": "Expression"
					}
				},
				"inputs": [
					{
						"referenceName": "Dataverse_DS",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_blobstorage",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}