						"type": "CommonDataServiceForAppsSource"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "craf2_ncnr",
									"type": "String"
								},
								"sink": {
									"name": "NCNR",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "craf2_customorstandard",
									"type": "String"
								},
								"sink": {
									"name": "Business_Unit_ID",
