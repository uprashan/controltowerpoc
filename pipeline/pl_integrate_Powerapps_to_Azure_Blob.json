									"name": "craf2_subconsite",
									"type": "String"
								},
								"sink": {
									"name": "Subcon_site",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "craf2_criticalshortageflag",
									"type": "String"
								},
								"sink": {
									"name": "Critical_Shortage_Flag",
									"type": "String"
